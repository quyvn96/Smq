<!DOCTYPE html>
<html>
<head>
    <title>@ViewBag.Title</title>
    <meta name="MetaKeyword" content="@ViewBag.MetaKeyword" />
    <meta name="MetaDescription" content="@ViewBag.MetaDescription" />
    @Styles.Render("~/css/base")
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1">
    <script type="application/x-javascript"> addEventListener("load", function() { setTimeout(hideURLbar, 0); }, false); function hideURLbar(){ window.scrollTo(0,1); } </script>
    <!--fonts-->
    <link href='http://fonts.googleapis.com/css?family=Open+Sans:400,300,600,700,800' rel='stylesheet' type='text/css'>

</head>
<body>
    <!--header-->
    @{Html.RenderAction("Header", "Home");}
    <!---->
    <div class="container">
       @RenderBody()
        @{Html.RenderAction("Category", "Home");}
    </div>
    @{Html.RenderAction("Footer", "Home");}
    <div class="clearfix"> </div>
    <!--script-->
    @Scripts.Render("~/js/jquery")
    @*<script src="~/Scripts/ActiveScript.js"></script>*@
    @Scripts.Render("~/js/plugins")
    @RenderSection("footerJS",required:false)
</body>
</html>